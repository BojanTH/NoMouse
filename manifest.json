{
"manifest_version": 2,
  "name": "NoMouse",
  "version": "1.01",

  "description": "Adds a basic Vim-like navigation. Supported shortcuts: j, k, gg, G",

  "icons": {
    "48": "icons/nm-48.png",
    "96": "icons/nm-96.png"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["node_modules/mousetrap/mousetrap.min.js","main.js"]
    }
  ]
}
